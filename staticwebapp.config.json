{
  "routes": [
    {
      "route": "/",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/about",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/contact",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/profile*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/health",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "globalHeaders": {
    "cache-control": "no-store, must-revalidate"
  },
  "trailingSlash": "auto"
}
